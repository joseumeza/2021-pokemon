<div class="card pokecard container-fluid" style="width: 18rem">
  <img src= <%= pokemon.sprites.front_default %>  class="card-img-top" alt=<%="front-" + pokemon.name %> />
  <div class="card-body">
    <h5 class="card-title"><%= pokemon.name.toUpperCase() %></h5>
    <div class="container">
        <div class="row">
            <div class="col">ID: <%= pokemon.id %> </div>
            <div class="col">Weight: <%= pokemon.weight %> </div>
        </div>
    </div>
    <a href="#" class="btn btn-primary">Go somewhere</a>
  </div>
</div>
